<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Colours-->
    <Color x:Key="DarkGreen"    >#11a5a7    </Color>
    <Color x:Key="Green"        >#92d1c0    </Color>
    <Color x:Key="LightGreen"   >#c1d8b1    </Color>
    <Color x:Key="LighterGreen" >#d4e7cb    </Color>
    <Color x:Key="OffWhite"     >#e9ebea    </Color>
    <Color x:Key="Grey"         >#6a6f69    </Color>
    
    <!--Solid Brushes-->
    <SolidColorBrush x:Key="DarkGreenBrush"     Color="{StaticResource DarkGreen}"      />
    <SolidColorBrush x:Key="GreenBrush"         Color="{StaticResource Green}"          />
    <SolidColorBrush x:Key="LightGreenBrush"    Color="{StaticResource LightGreen}"     />
    <SolidColorBrush x:Key="LighterGreenBrush"  Color="{StaticResource LighterGreen}"   />
    <SolidColorBrush x:Key="OffWhiteBrush"      Color="{StaticResource OffWhite}"       />
    <SolidColorBrush x:Key="GreyBrush"          Color="{StaticResource Grey}"           />
</ResourceDictionary>